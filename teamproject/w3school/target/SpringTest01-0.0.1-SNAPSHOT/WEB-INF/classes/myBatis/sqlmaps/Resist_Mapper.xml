         <result property="freen_mail"     column="FREEN_MAIL"     javaType="string"/>
         <result property="freeln_phone"   column="FREELN_PHONE"   javaType="string"/>
        
         <result property="hope_place"     column="HOPE_PLACE"     javaType="string"/>
